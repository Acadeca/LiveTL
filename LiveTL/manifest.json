{
    "name": "LiveTL",
    "description": "Get live translations for YouTube streams, crowdsourced from bilingual viewers!",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/live_chat*",
                "https://www.youtube.com/watch*"
            ],
            "js": [
                "frame.js"
            ],
            "all_frames": true
        }
    ],
    "browser_action": {}
}