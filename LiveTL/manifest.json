{
    "name": "LiveTL",
    "description": "Get live translations for YouTube streams, crowdsourced from bilingual viewers!",
    "version": "0.1.0",
    "manifest_version": 2,
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "frame.js"
            ],
            "all_frames": true
        }
    ],
    "browser_action": {},
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "./icons/16x16.png",
        "48": "./icons/48x48.png",
        "128": "./icons/128x128.png"
    }
}